package com.springJPA.data;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

@Entity
@Table (name="Student")
public class Student {
	@Id
	int id;
	String name;
	String stream;
	int marks;
	
	Student(){}

	public Student(int id, String name, String stream, int marks) {
		super();
		this.id = id;
		this.name = name;
		this.stream = stream;
		this.marks = marks;
	}
	
	

}
